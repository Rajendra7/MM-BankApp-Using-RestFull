package com.moneymoneybank.app.service;

import java.time.LocalDate;
import java.util.List;

import com.moneymoneybank.app.entity.Transaction;

public interface TransactionService {
	List<Transaction> getStatement(LocalDate startDate, LocalDate endDate);

	Double withdraw(int accountNumber, double amount, double currentBalance, String TrasactionDetails);

	Double deposit(int accountNumber, double amount, double currentBalance, String TrasactionDetails);

	Double[] fundTransfer(int senderAccountNumber, int receiverAccountNumber, double amount, double currentBalance,
			String TrasactionDetails);

}